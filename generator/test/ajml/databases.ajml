<?xml version="1.0" encoding="UTF-8"?>
<ajml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <application name="chat">
    <databases>
      <database id="room_database" tablename="room">
	<property name="name" type="string"/>
	<property name="message" type="ref" multiplicity="*" ref="message"/>
	<init>
	  <item><property name="name"><string>ルーム１</string></property></item>
	  <item><property name="name"><string>ルーム２</string></property></item>
	  <item><property name="name"><string>ルーム３</string></property></item>
	  <item><property name="name"><string>ルーム４</string></property></item>
	  <item><property name="name"><string>ルーム５</string></property></item>
	</init>
      </database>
      <database id="message_database" tablename="message">
	<property name="user" type="string"/>
	<property name="message" type="string"/>
	<property name="posted" type="datetime"/>
	<property name="room" type="ref" multiplicity="1" ref="room"/>
      </database>
<!--      <driver id="">
	<driverClass/>
	<databaseName/>
	<user/>
	<password/>
      </driver> -->
    </databases>
  </application>
</ajml>
